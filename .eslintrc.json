{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "experimantalDecorators": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react"],
  "extends": "airbnb",
  "rules": {
    "implicit-arrow-linebreak": [0, "beside"],
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
